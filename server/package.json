{
  "name": "glocalcloudapp-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
  "build": "tsc -p tsconfig.json",
  "migrate": "tsx src/migrate.ts",
  "seed": "tsx src/seed.ts",
  "start:prod": "npm run build && node dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
  "@types/cors": "^2.8.17",
  "@types/pg": "^8.11.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
