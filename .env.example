# Environment variables for the Expo app
# Copy this file to .env and fill in real values.

GOOGLE_MAPS_API_KEY=YOUR_ANDROID_GOOGLE_MAPS_API_KEY_HERE

# Server configuration
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
DATABASE_SSL=require
APP_API_KEY=your-secure-api-key-here

# CMS Cache TTL in milliseconds
CMS_CACHE_TTL_MS=15000

# Geofence catch-up configuration
GEOFENCE_BATCH_SIZE=100
GEOFENCE_LOOKBACK_MINUTES=30
